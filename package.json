{
  "name": "@expo/browser-polyfill",
  "version": "0.0.1-alpha.0",
  "description": "Browser polyfill for making React Native compatible with web libs like pixi.js, three.js, phaser.js",
  "homepage": "https://github.com/expo/browser-polyfill#readme",
  "bugs": {
    "url": "https://github.com/expo/browser-polyfill/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/browser-polyfill.git"
  },
  "keywords": [
    "expo",
    "browser",
    "polyfill",
    "react-native",
    "react",
    "web",
    "dom",
    "document",
    "shim"
  ],
  "private": false,
  "author": {
    "email": "bacon@expo.io",
    "name": "Evan Bacon"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "pre-push": [
    "lint"
  ],
  "directories": {
    "example": "examples",
    "lib": "src"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "npm run build",
    "prepush": "npm run lint",
    "lint:example": "eslint example/",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "sync-example": "npm run build && rsync -rv dist example/node_modules/@expo/browser-polyfill && rsync -rv package.json example/node_modules/@expo/browser-polyfill && rm -rf example/node_modules/gl-matrix/.babelrc",
    "postinstall": "rm -rf ../../gl-matrix/.babelrc"
  },
  "peerDependencies": {
    "expo": "^28.0.0",
    "react": "^16.3.0",
    "react-native": "*"
  },
  "devDependencies": {
    "babel-preset-expo": "^4.0.0",
    "eslint": "^5.1.0",
    "eslint-config-universe": "^1.0.6",
    "eslint-plugin-jest": "^21.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-expo": "^29.0.0",
    "prettier": "^1.7.4",
    "rollup": "0.63.4",
    "rollup-plugin-commonjs": "9.1.4",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-typescript2": "0.16.1"
  },
  "dependencies": {
    "gl-matrix": "2.6.1",
    "adaptive-bezier-curve": "1.0.3",
    "adaptive-quadratic-curve": "1.0.2",
    "color-parser": "0.1.0",
    "earcut": "2.1.3",
    "extrude-polyline": "1.0.6",
    "fbemitter": "^2.1.1",
    "react-native-console-time-polyfill": "^0.0.6",
    "string-format": "2.0.0",
    "text-encoding": "^0.6.4",
    "xmldom-qsa": "^1.0.3"
  }
}
